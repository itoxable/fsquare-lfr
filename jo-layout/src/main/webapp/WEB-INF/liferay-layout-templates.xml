			<template-path>/max.tpl</template-path>
			<wap-template-path>/max.wap.tpl</wap-template-path>
			<thumbnail-path>/max.png</thumbnail-path>
		</layout-template>
	</standard>
	<custom>
		<layout-template id="jo-layout" name="Jo Layout">
			<template-path>/jo-layout.tpl</template-path>
